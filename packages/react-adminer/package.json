{
  "name": "react-adminer",
  "version": "0.0.34",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JaLe29/react-adminer.git"
  },
  "types": "./dist/index.d.ts",
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "publish": "yarn clean && yarn build:prod && yarn workspace react-adminer npm publish --otp \"%NPM_PUBLISH_TOKEN%\"",
    "watch": "yarn build:deps && yarn build:prod",
    "watch:types": "tsc --watch --project tsconfig.watch.json",
    "build:prod": "yarn clean && vite build --mode release && yarn build:types",
    "build:types": "tsc --project tsconfig.watch.json --declarationDir ./dist/types",
    "build": "tsc --build tsconfig.build.json",
    "build:dev": "yarn build:deps && tsc --build tsconfig.build.json",
    "clean": "rimraf dist && rimraf .cache && rimraf node_modules",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-is": "^17.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.0.1",
    "antd": "^4.22.4",
    "eslint": "^8.21.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.9"
  },
  "peerDependencies": {
    "@ant-design/icons": ">= 4.7.0",
    "antd": ">= 4.21.0",
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "dependencies": {
    "moment": "^2.29.4"
  }
}
